<set>
    <broadcast>
        <action>com.atakmap.app.LINK_LINE</action>
        <extras>
            <string key='firstUID'>@{deviceUid}</string>
            <string key='secondUID'>${uid}</string>
        </extras>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.UNFOCUS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_DETAILS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_MENU</action>
    </broadcast>
</set>
