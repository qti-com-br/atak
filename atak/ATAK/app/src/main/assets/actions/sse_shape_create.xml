<set>
    <broadcast>
        <action>com.atakmap.android.maps.UNFOCUS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_DETAILS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_MENU</action>
    </broadcast>
    
    <broadcast>
        <action>com.atakmap.android.toolbars.SSE_UNDO_TOOLBAR</action>
    </broadcast>
    
    <broadcast>
        <action>com.atakmap.android.maps.toolbar.SET_TOOLBAR</action>
        <extras>
            <string key='toolbar'>com.atakmap.android.toolbars.SensitiveSiteExploitationToolbar</string>
        </extras>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.toolbar.BEGIN_TOOL</action>
        <extras>
            <string key='tool'>com.atakmap.android.sse.GRGCreationSSETool</string>
            <string key='entity_id'>${entity_id}</string>
            <string key='uid'>${uid}</string>
        </extras>
    </broadcast>
</set>