<set>
    <broadcast>
        <action>com.atakmap.android.maps.UNFOCUS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_DETAILS</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.HIDE_MENU</action>
    </broadcast>
    <broadcast>
        <action>com.atakmap.android.maps.PLACE</action>
        <extras>
            <string key='point'>@{targetPoint}</string>
            <string key='atself'>true</string>
            <string key='type'>a-u-G</string>
            <string key='how'>h-g-i-g-o</string>
            <string key='show_new_radial'>true</string>
        </extras>
    </broadcast>
</set>
